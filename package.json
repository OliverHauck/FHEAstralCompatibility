{
  "name": "astral-compatibility",
  "version": "1.0.0",
  "description": "Privacy-Preserving Zodiac Compatibility Matching using FHE",
  "homepage": "https://oliverhauck.github.io/FHEAstralCompatibility/",
  "main": "index.html",
  "scripts": {
    "dev": "npx http-server . -p 3000 -c-1 --cors",
    "build": "echo 'Static site - no build step required'",
    "start": "npx http-server . -p 8080 -c-1 --cors",
    "predeploy": "npm run build",
    "deploy:gh-pages": "gh-pages -d .",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:mock": "hardhat run scripts/deploy-mock.js --network sepolia",
    "verify": "hardhat run scripts/verify.js --network sepolia",
    "interact": "hardhat run scripts/interact.js --network sepolia",
    "simulate": "hardhat run scripts/simulate.js --network sepolia",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write 'contracts/**/*.sol' 'scripts/**/*.js' 'test/**/*.js'",
    "prepare": "husky"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "encryption",
    "privacy",
    "web3",
    "blockchain",
    "sdk",
    "react",
    "vue",
    "nextjs"
  ],
  "author": "Astral Compatibility Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OliverHauck/FHEAstralCompatibility.git"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "gh-pages": "^6.1.0",
    "hardhat": "^2.19.0",
    "hardhat-contract-sizer": "^2.10.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "solhint": "^6.0.1"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "@zama-fhe/oracle-solidity": "^0.2.0",
    "fhevm": "^0.6.2"
  }
}
